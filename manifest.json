{
    "manifest_version": 3,
    "name": "Text Replacer",
    "version": "1.0",
    "description": "A Chrome extension to replace text on a website",
    "permissions": ["storage","activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  